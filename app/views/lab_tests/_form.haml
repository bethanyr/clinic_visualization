%h1 Add a Lab Test

= simple_form_for(@lab_test, html: {class: 'form-vertical' }) do |f|
  = f.input :start_date
  = f.input :name
  = f.input :result
  = f.input :value
  = f.input :units
  = f.input :description
  = f.input :long_name
  = f.input :medical_condition_id
  = f.submit 'Save', :class => 'btn btn-primary'
